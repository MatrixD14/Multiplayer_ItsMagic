class moveVision extends Component { //[I-S]LN=0;[I-E]
  private Vector2 vision, visionMouse, joy; //[I-S]LN=1;[I-E]
  private Characterbody charac = null; //[I-S]LN=2;[I-E]
  private float speedJ = 3, speedS = 1f, camv, camh; //[I-S]LN=3;[I-E]
  public float x, y; //[I-S]LN=4;[I-E]
  @Singleton //[I-S]LN=5;[I-E]
  private criente1 serve; //[I-S]LN=6;[I-E]
  void start() { //[I-S]LN=7;[I-E]
    charac = myObject.getPhysics().getPhysicsEntity(); //[I-S]LN=8;[I-E]
    joy = Input.getAxisValue("joy"); //[I-S]LN=9;[I-E]
    vision = Input.getAxisValue("vision"); //[I-S]LN=10;[I-E]
  } //[I-S]LN=11;[I-E]
 //[I-S]LN=12;[I-E]
  void repeat() { //[I-S]LN=13;[I-E]
    visionMouse = new Vector2(Input.mouse.getSlideX(), Input.mouse.getSlideY()); //[I-S]LN=14;[I-E]
    x = Input.mouse.getSlideX(); //[I-S]LN=15;[I-E]
    y = Input.mouse.getSlideY(); //[I-S]LN=16;[I-E]
    if (key("w") || key("s") || key("a") || key("d")) { //[I-S]LN=17;[I-E]
      movekey(); //[I-S]LN=18;[I-E]
    } else move(joy.x * speedJ, joy.y * speedJ); //[I-S]LN=19;[I-E]
 //[I-S]LN=20;[I-E]
    /*    if (Input.mouse.isConnected() && !Input.getTouch(0).isPressed()) { //[I-S]LN=21;[I-E]
      vision(x < -20 && x > 20 ? 0 : x * speedS, y < -5 && y > 5 ? 0 : y * speedS); //[I-S]LN=22;[I-E]
    } else */ //[I-S]LN=23;[I-E]
    vision(vision.x * speedS, vision.y * speedS); //[I-S]LN=24;[I-E]
  } //[I-S]LN=25;[I-E]
 //[I-S]LN=26;[I-E]
  private void movekey() { //[I-S]LN=27;[I-E]
    float x = 0, y = 0; //[I-S]LN=28;[I-E]
    if (key("w")) y = +1; //[I-S]LN=29;[I-E]
    if (key("s")) y = -1; //[I-S]LN=30;[I-E]
    if (key("a")) x = -1; //[I-S]LN=31;[I-E]
    if (key("d")) x = +1; //[I-S]LN=32;[I-E]
    move(x * speedJ, y * speedJ); //[I-S]LN=33;[I-E]
  }  //[I-S]LN=34;[I-E]
 //[I-S]LN=35;[I-E]
  private boolean key(String key) { //[I-S]LN=36;[I-E]
    if (Input.keyboard.isKeyPressed(key)) return true; //[I-S]LN=37;[I-E]
    return false; //[I-S]LN=38;[I-E]
  } //[I-S]LN=39;[I-E]
 //[I-S]LN=40;[I-E]
  private void move(float x, float y) { //[I-S]LN=41;[I-E]
    charac.setSpeed(-x, -y); //[I-S]LN=42;[I-E]
    serve.upMovePlay(x,0,y); //[I-S]LN=43;[I-E]
  } //[I-S]LN=44;[I-E]
 //[I-S]LN=45;[I-E]
  private void vision(float x, float y) { //[I-S]LN=46;[I-E]
    camh = Math.clamp(-80, camh += y, 45); //[I-S]LN=47;[I-E]
    myObject.findChildObject("vision").getRotation().selfLookTo(new Vector3(0, Math.sin(-camh), Math.cos(-camh))); //[I-S]LN=48;[I-E]
    camv += x; //[I-S]LN=49;[I-E]
    myObject.getRotation().selfLookTo(new Vector3(Math.sin(-camv), 0, Math.cos(-camv))); //[I-S]LN=50;[I-E]
  } //[I-S]LN=51;[I-E]
} //[I-S]LN=52;[I-E]